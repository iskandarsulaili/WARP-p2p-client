{
  "coordinator": {
    "rest_api_url": "http://localhost:8001/api/v1",
    "websocket_url": "ws://localhost:8001/api/v1/signaling/ws",
    "timeout_seconds": 30,
    "reconnect_max_attempts": 5,
    "reconnect_backoff_ms": 1000
  },
  "webrtc": {
    "stun_servers": [
      "stun:stun.l.google.com:19302",
      "stun:stun1.l.google.com:19302"
    ],
    "turn_servers": [],
    "ice_transport_policy": "all",
    "bundle_policy": "balanced",
    "rtcp_mux_policy": "require",
    "enable_dtls": true,
    "enable_rtp_data_channels": false
  },
  "p2p": {
    "enabled": true,
    "max_peers": 50,
    "max_packet_size_bytes": 65536,
    "max_bandwidth_mbps": 100,
    "target_bitrate_kbps": 5000,
    "enable_congestion_control": true,
    "packet_queue_size": 1000
  },
  "security": {
    "enable_encryption": true,
    "enable_authentication": true,
    "api_key": "",
    "jwt_token": "",
    "certificate_validation": true,
    "tls_version": "1.3"
  },
  "logging": {
    "level": "info",
    "file": "p2p_dll.log",
    "max_file_size_mb": 10,
    "max_files": 5,
    "console_output": true,
    "async_logging": true
  },
  "zones": {
    "p2p_enabled_zones": [
      "prontera",
      "geffen",
      "payon",
      "morocc",
      "alberta",
      "aldebaran",
      "izlude"
    ],
    "fallback_on_failure": true,
    "zone_transition_timeout_ms": 5000
  },
  "performance": {
    "worker_threads": 4,
    "io_thread_pool_size": 2,
    "enable_packet_batching": true,
    "packet_batch_size": 10,
    "packet_batch_timeout_ms": 5
  },
  "host": {
    "enable_hosting": false,
    "max_players": 50,
    "max_zones": 5,
    "heartbeat_interval_seconds": 30,
    "quality_report_interval_seconds": 60
  }
}
